{
    "version": 2,
	"functions": {
		"api/**/*.py": {
		  "excludeFiles": ["**/*.html","**/*.pyc","**/*.pyo","**/*.pyd","__pycache__/**",".pytest_cache/**"]
		}
    },
    "builds": [
        {
            "src": "./main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}